{
  "name": " spider-ui",
  "version": "1.0.0",
  "private": true,
  "config": {
    "vd": "/",
    "port": 3000,
    "env": "development"
  },
  "scripts": {
    "start": "nodemon --inspect ./bin/www",
    "dev": "igame dev",
    "ssr": "igame ssr",
    "init": "igame init",
    "pro": "igame pro",
    "rename": "igame rename",
    "pm2": "pm2 startOrRestart ./config/pm2/run.config.js",
    "pm2:fat": "cross-env NODE_ENV=fat pm2 startOrRestart ./config/pm2/run.config.js --env fat",
    "pm2:production": "cross-env NODE_ENV=production pm2 startOrRestart ./config/pm2/run.config.js --env production",
    "precommit": "node ./precommit.js"
  },
  "pre-commit": [
    "precommit"
  ],
  "dependencies": {
    "babel-plugin-import": "1.10.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "~1.13.2",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cookie-parser": "~1.4.3",
    "cross-env": "^2.0.1",
    "debug": "~2.6.9",
    "ejs": "^2.3.4",
    "express": "~4.16.0",
    "fs.extra": "^1.3.2",
    "helmet": "^3.1.0",
    "igame_base_libs": "0.7.1",
    "iqiyi_game_wp_cli": "1.3.0",
    "node-fetch": "^2.6.1",
    "pm2": "2.4.6",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "whatwg-fetch": "^2.0.4",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^3.3.2"
  },
  "devDependencies": {
    "cross-env": "^2.0.1",
    "nodemon": "^1.17.4",
    "pre-commit": "^1.2.2"
  }
}
